{
	"description": "hides untrusted and non-vip uploaders",
	"name": "tpbjs",

	"manifest_version": 2,
	"version": "1.0",

	"content_scripts": [
		{
			"matches": ["*://thepiratebay.org/*"],
			"js": [
				"jquery/jquery-2.2.4.min.js",
				"content_scripts/tpb.js"
			]
		}
	],
	"applications": {
		"gecko": {
			"id": "https://github.com/wesleyem/tpbjs/",
			"strict_min_version": "42.0"
		}
	}

}